node {  
  stage('SCM'){
  git url: 'https://github.com/Josue-lab/Script-Example.git'
  }
  stage('Hello World') {
    echo "Hello World" 
  } 
  stage('Step1'){
      bat "step1.bat"
  }
    stage('Step2'){
      bat "step2.bat"
  }
    stage('Sanity check') {
          input "Does the staging environment look ok?"
          bat 'deploy production'
  }
   
}
